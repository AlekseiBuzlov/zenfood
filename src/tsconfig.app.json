{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "../out-tsc/app",
    "types": []
  },
  "exclude": [
    "test.ts",
    "**/*.spec.ts"
  ],
  "paths": {
      "@reducers/*": [
        "store/reducers/*"
      ],
      "@services/*": [
        "services/*"
      ],
      "@actions/*": [
        "store/actions/*"
      ],
      "@effects/*": [
        "store/effects/*"
      ],
      "@core/*": [
        "core/*"
      ],
      "@environment/*": [
        "environments/*"
      ],
      "@common/*": [
        "components/common/*"
      ],
      "state": [
        "store/reducers/reducers.ts"
      ]
    }
}
